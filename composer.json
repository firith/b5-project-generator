{
    "name": "firith/b5-project-generator",
    "description": "Generate b5 project.",
    "keywords": [
        "b5",
        "laravel",
        "symfony"
    ],
    "homepage": "https://github.com/firith/b5-project-generator",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/firith/b5-project-generator/issues",
        "source": "https://github.com/firith/b5-project-generator"
    },
    "authors": [
        {
            "name": "Bal√°zs Simon",
            "email": "balazs@firith.dev"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "laravel-zero/framework": "^8.0",
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^0.3",
        "symfony/process": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "builds/b5-project"
    ]
}
